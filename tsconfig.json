{
    "compilerOptions": {
        "target": "es2018",
        "module": "commonjs",
        "outDir": "./out",
        "sourceMap": true,
		"strict": true,
		"alwaysStrict": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictPropertyInitialization": false,
        "experimentalDecorators": true
    },
    "include": [
        "src/**/*"
    ]
}
